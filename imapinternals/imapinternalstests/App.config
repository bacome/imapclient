<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true"></trace>
    <sources>
      <source name="work.bacome.imapinternalstests" switchName="traceswitch">
        <listeners>
          <remove name="Default"></remove>
          <add name="file"></add>
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="traceswitch" value="Verbose"/>
    </switches>
    <sharedListeners>
      <add name="file"
           type="System.Diagnostics.TextWriterTraceListener"
           initializeData="imapinternalstests.txt">
      </add>
    </sharedListeners>
  </system.diagnostics>
</configuration>